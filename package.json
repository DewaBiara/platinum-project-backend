  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./index.js",
    "develop": "nodemon ./index.js",
    "db:create": "npx sequelize db:create",
    "seed:users": "npx sequelize-cli seed:generate --name demo-user",
    "seed:airport": "npx sequelize-cli seed:generate --name demo-airport",
    "seed:address": "npx sequelize-cli seed:generate --name demo-address",
    "seed:role": "npx sequelize-cli seed:generate --name demo-role",
    "seed:userbooking": "npx sequelize-cli seed:generate --name demo-userbooking",
    "seed:booking": "npx sequelize-cli seed:generate --name demo-booking",
    "seed:wishlist": "npx sequelize-cli seed:generate --name demo-wishlist",
    "seed:ticket": "npx sequelize-cli seed:generate --name demo-ticket",
    "seed:flight": "npx sequelize-cli seed:generate --name demo-flight",
    "seed:flighttype": "npx sequelize-cli seed:generate --name demo-flighttype",
    "seed:classtype": "npx sequelize-cli seed:generate --name demo-classtype",
    "seed:passanger": "npx sequelize-cli seed:generate --name demo-passanger",
    "seed:history": "npx sequelize-cli seed:generate --name demo-history",
    "seed:": "npx sequelize-cli seed:generate --name demo-",
    "model-users": "npx sequelize-cli model:generate --name Users --attributes firstname:string,lastname:string,gender:string,email:string,phone:string,birthdate:date,address_id:integer,password:string,role_id:integer,pictures:string,access_token:text,refresh_token:text",
    "model-airport": "npx sequelize-cli model:generate --name Airport --attributes name:string,code:string,city:string,country:string,terminal:string,status:boolean",
    "model-address": "npx sequelize-cli model:generate --name Address --attributes homeAddress:string,city:string,province:string,country:string",
    "model-role": "npx sequelize-cli model:generate --name Role --attributes roleName:string",
    "model-userbooking": "npx sequelize-cli model:generate --name UserBooking --attributes user_id:integer,booking_id:integer",
    "model-booking": "npx sequelize-cli model:generate --name Booking --attributes ticket_id:integer,passanger_id:integer,isBooking:boolean",
    "model-wishlist": "npx sequelize-cli model:generate --name Wishlist --attributes user_id:integer,ticket_id:integer,isWishlist:boolean",
    "model-ticket": "npx sequelize-cli model:generate --name Ticket --attributes flight_id:integer,class_id:integer,price:float,country:string,passanger_ammount:string",
    "model-flight": "npx sequelize-cli model:generate --name Flight --attributes departureAirport:integer,arrivalAirport:integer,departureDate:date,arrivalDate:date,departureTime:time,arrivalTime:time,flightType:integer,planeName:string",
    "model-flighttype": "npx sequelize-cli model:generate --name FlightType --attributes type:string",
    "model-classtype": "npx sequelize-cli model:generate --name ClassType --attributes type:string",
    "model-passanger": "npx sequelize-cli model:generate --name Passanger --attributes name:string,email:string,age:integer,identityType:string,identityNumber:string,booking_id:integer",
    "model-history": "npx sequelize-cli model:generate --name History --attributes id_ticket:integer,id_users:integer",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop"
  },
